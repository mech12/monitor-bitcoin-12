{
  "name": "01Cnode",
  "version": "0.1.10",
  "description": "web frontend for bitcoind, a decentralised block explorer",
  "main": "server.js",
  "scripts": {
    "test": "find ./tests -name '*.js' | xargs -n 1 ./node_modules/mocha/bin/mocha -R spec",
    "watch": "watchify js/app.js --poll=1000 -o static/bundle.js -vvv",
    "build": "browserify js/app.js -t envify | uglifyjs -mc warnings=false > static/bundle.js",
    "check-updates": "./node_modules/npm-check-updates/bin/npm-check-updates",
    "start": "nodemon server.js",
    "buildAndRun": "npm run build && npm start",
    "start2": "nodemon --watch server.js --watch js/api 2> npm_start.error| tee -a npm_start.log"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KanoczTomas/01Cnode.git"
  },
  "keywords": [
    "12ships",
    "bitcoin",
    "angularjs",
    "mempool",
    "bitcoin-node",
    "realtime",
    "blockexplorer",
    "zeromq",
    "web-frontend",
    "nodejs",
    "rest",
    "bootstrap"
  ],
  "author": "Tomas Kanocz",
